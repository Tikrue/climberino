{
  "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "jsx-no-lambda": {
      "severity": "warning"
    },
    "ordered-imports": false,
    "jsx-boolean-value": false,
    "member-access": false,
    "no-console": [true, "warning"]
  }
}
